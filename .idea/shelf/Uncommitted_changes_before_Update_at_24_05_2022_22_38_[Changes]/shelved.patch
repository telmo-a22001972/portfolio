<+>{%  extends 'portfolio/layout.html' %}\r\n\r\n{% block main %}\r\n    {% load static %}\r\n    <link href=\"{% static 'portfolio/css/styles_posts.css'  %}\" rel=\"stylesheet\" type=\"text/css\">\r\n\r\n\t<h1>Blog</h1>\r\n\r\n    <div class=\"btn_div\">\r\n        <a href=\"{% url 'portfolio:criar_post' %}\" class=\"btn\">+ Novo Post</a>\r\n    </div>\r\n\r\n    <div class=\"posts_container\">\r\n\r\n        {% for post in posts %}\r\n\r\n            <div class=\"blog_post\">\r\n                <h2>{{post.titulo}}</h2>\r\n                <br>\r\n                <br>\r\n                <p>{{post.descricao}}</p>\r\n                <br>\r\n                <br>\r\n                <br>\r\n                <div class=\"post_criacao\">\r\n                    <p>Criado: {{post.criacao.day}}/{{post.criacao.month}}/{{post.criacao.year}} </p>\r\n                </div>\r\n                <div class=\"post_autor\">\r\n                    <p>Autor: {{ post.autor }}</p>\r\n                </div>\r\n            </div>\r\n        {% endfor %}\r\n\r\n    </div>\r\n\r\n{% endblock %}
